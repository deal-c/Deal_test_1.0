<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_huan_magic"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    tools:context="com.first.yuliang.deal_community.home_button_activity.huan_magic">
    <android.support.v7.widget.Toolbar
        android:id="@+id/qijihuan"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="@color/bgcolor">

        <TextView
            android:layout_gravity="center"
            android:layout_centerInParent="true"
            android:layout_marginLeft="10dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="奇迹换"
            android:textSize="20sp"
            android:textColor="#fff"/>
        <ImageView
            android:id="@+id/qijihuan_returen"
            android:src="@drawable/return_"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="left"/>
    </android.support.v7.widget.Toolbar>
<com.first.yuliang.deal_community.Util.GifView
    android:id="@+id/circlepic"
    android:layout_width="200dp"
    android:layout_height="200dp"
    android:layout_centerInParent="true"
    />
    <ImageView
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_below="@+id/qijihuan"
        android:src="@drawable/loadddd"/>



</RelativeLayout>
